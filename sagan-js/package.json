{
  "name": "spring.io",
  "description": "node.js packages for spring.io UI.",
  "dependencies": {
    "cram": "~0",
    "grunt-cli": "~0.1.11",
    "grunt": "~0.4.2",
    "grunt-contrib-cssmin": "~0.7.0"
  },
  "devDependencies": {
    "when": "~2",
    "buster": "~0.7",
    "bower": "~1",
    "jshint": "~2.1",
    "wd": "~0.2",
    "phantomjs": "~1"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "grunt && npm run cram",
    "cram": "node node_modules/cram dev/run.js --output dist/run.js --include curl/loader/legacy --include curl/loader/cjsm11",
    "test": "jshint . && buster-test -c test/unit/buster.js",
    "phantom": "buster-test -c test/scenario/buster.js -g scenario-phantom",
    "firefox": "buster-test -c test/scenario/buster.js -g scenario-firefox",
    "scenario": "buster-test -c test/scenario/buster.js -g scenario",
    "prepublish": "bower install"
  }
}
