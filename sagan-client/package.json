{
  "name": "spring.io",
  "description": "node.js packages for spring.io UI.",
  "dependencies": {

  },
  "devDependencies": {
    "bower": "~1.3.1",
    "cram": "~0.8.1",
    "event-stream":"3.1.0",
    "gulp": "~3.5.0",
    "gulp-util": "latest",
    "gulp-filter": "~0.3.0",
    "gulp-cram": "~0.0.2",
    "gulp-minify-css": "~0.3.0",
    "gulp-uglify": "~0.2.0",
    "gulp-imagemin": "~0.1.5",
    "gulp-bower-src": "~0.0.1",
    "gulp-watch": "~0.5.0",
    "gulp-changed":"~0.1.0",
    "when": "~2",
    "buster": "~0.7",
    "jshint": "~2.1",
    "wd": "0.2.10",
    "phantomjs": "~1"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "bower install --config.interactive=false",
    "build": "gulp build",
    "watch": "gulp watch",
    "cram": "cram src/run.js cram-config.json --output build/tmp/static/run.js --include curl/loader/legacy --include curl/loader/cjsm11",
    "test": "jshint . && buster-test -c test/unit/buster.js",
    "phantom": "buster-test -c test/scenario/buster.js -g scenario-phantom",
    "firefox": "buster-test -c test/scenario/buster.js -g scenario-firefox",
    "scenario": "buster-test -c test/scenario/buster.js -g scenario"
  }
}
