{
  "name": "spring.io",
  "description": "node.js packages for spring.io UI.",
  "dependencies": {

  },
  "devDependencies": {
      "cram": "~0.7.12",
      "grunt-cli": "~0.1.11",
      "grunt": "~0.4.2",
      "grunt-contrib-cssmin": "~0.7.0",
      "grunt-bower-task": "~0.3",
      "grunt-contrib-uglify": "~0.2.7",
      "grunt-contrib-copy": "~0.5.0",
    "when": "~2",
    "buster": "~0.7",
    "bower": "~1",
    "jshint": "~2.1",
    "wd": "~0.2",
    "phantomjs": "~1"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run cram && grunt optimize",
    "dump": "npm run cram && grunt dump",
    "cram": "node node_modules/cram src/run.js cram-config.json --output build/tmp/static/run.js --include curl/loader/legacy --include curl/loader/cjsm11",
    "test": "jshint . && buster-test -c test/unit/buster.js",
    "phantom": "buster-test -c test/scenario/buster.js -g scenario-phantom",
    "firefox": "buster-test -c test/scenario/buster.js -g scenario-firefox",
    "scenario": "buster-test -c test/scenario/buster.js -g scenario",
    "prepublish": "grunt"
  }
}
