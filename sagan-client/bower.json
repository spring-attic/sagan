{
    "name": "spring.io",
    "description": "bower components for spring.io UI.",
    "dependencies": {
        "when": "~2",
        "meld": "~1",
        "most": "~0.2",
        "curl": "~0.8",
        "poly": "~0",
        "jquery": "1.8.3",
        "bootstrap": "~2.3",
        "bootstrap-datetimepicker": "0.0.10",
        "font-awesome": "3.2.1",
        "leaflet": "~0.5",
        "zeroclipboard": "1.1.7",
        "google-code-prettify": "~1.0"
    },
    "overrides": {
        "jquery": {
            "main": "jquery.min.js"
        },
        "curl": {
            "main": "./dist/curl/curl.js"
        },
        "meld": {
            "main": "meld.js"
        },
        "most": {
            "main": "most.js"
        },
        "bootstrap": {
            "main": [
                "./docs/assets/js/bootstrap.js",
                "./docs/assets/css/bootstrap.css",
                "./docs/assets/img/*"
            ]
        },
        "bootstrap-datetimepicker": {
          "main": "js/bootstrap-datetimepicker.js"
        },
        "font-awesome": {
            "main" : "./font/*"
        },
        "leaflet": {
            "main" : "./dist/**"
        },
        "google-code-prettify": {
            "main": "./src/*"
        }
    }
}
