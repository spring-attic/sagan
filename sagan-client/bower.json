{
    "name": "spring.io",
    "description": "bower components for spring.io UI.",
    "dependencies": {
        "when": "~2",
        "meld": "~1",
        "most": "~0.2",
        "curl": "~0.8",
        "poly": "~0",
        "jquery": "1.8.3",
        "bootstrap": "~2.3",
        "bootstrap-datetimepicker": "0.0.10",
        "font-awesome": "3.2.1",
        "leaflet": "~0.5",
        "zeroclipboard": "1.1.7",
        "google-code-prettify": "~1.0"
    },
    "overrides": {
        "jquery": {
            "ignore": ["*.json"]
        },
        "curl": {
            "ignore": ["dist/**", "src/curl/**"]
        },
        "meld": {
            "ignore": ["aspect/**", "docs/**", "test/**"]
        },
        "most": {
            "ignore": ["test/**"]
        },
        "bootstrap": {
            "ignore": [
                "js/**", "img/**", "less/**", "docs/build/**",
                "docs/examples/**", "docs/templates/**", "docs/*.html"
            ]
        },
        "bootstrap-datetimepicker": {
            "ignore": ["build/**", "test/**"]
        },
        "font-awesome": {
            "ignore": ["css/**", "less/**", "scss/**", "src/**"]
        },
        "leaflet": {
            "ignore": ["build/**", "debug/**", "spec/**", "src/**"]
        },
        "google-code-prettify": {
            "ignore": ["styles/**"]
        }
    }
}
